{
  "name": "@neo4j-labs/temp-dbs",
  "repository": {
    "type": "git",
    "url": "https://github.com/neo4j-contrib/neo4j-temp-db"
  },
  "version": "0.2.3",
  "description": "expose a temporary database to run cyphers",
  "main": "dist/index.js",
  "scripts": {
    "prettier": "prettier --write",
    "test": "jest --testTimeout=30000",
    "build": "babel src --out-dir dist"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.13.10",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "jest-date-mock": "^1.0.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.21",
    "neo4j-driver": "^4.3.1",
    "neo4j-driver-core": "^4.3.1",
    "uuid": "^8.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "setupFiles": [
      "dotenv/config"
    ],
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  }
}
